<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yztour.web.mapper.MemberMapper">

    <select id="getMemberInfo" resultType="MemberVO">
        SELECT
              MEMBER_ID
            , USER_ID
            , PWD
            , NAME
            , BIRTHDAY
            , PHONENUMBER
            , EMAIL
            , ADDRESS
            , ADDRESS_DETAIL
            , ZIPCODE
            , ROUTE
            , STATUS
            , ADMIN_YN
        FROM Member
    </select>
</mapper>